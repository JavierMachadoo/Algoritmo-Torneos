# Configuración de la aplicación
# Copia este archivo como .env y actualiza los valores

# Clave secreta para JWT y sesiones (OBLIGATORIO en producción)
# Genera una con: python -c "import secrets; print(secrets.token_urlsafe(32))"
SECRET_KEY=your_secret_key_here

# Modo debug (False en producción)
DEBUG=False

# Credenciales de acceso (CAMBIAR EN PRODUCCIÓN - NO USAR ESTOS VALORES EN ENTORNOS REALES)
ADMIN_USERNAME=your_admin_username_here
ADMIN_PASSWORD=your_secure_admin_password_here
